                </defs>
            </svg>
        </a>

        <!-- navbar -->
        <nav class="font-['Sen']">
            <ul class="">
                <li class="inline p-4">
                    <a class="text-xl" href="#">ACCUEIL</a>
                </li>

                <li class="inline border-l-2 border-r-2 border-[#ffffff] p-2">
