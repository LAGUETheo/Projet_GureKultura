            </ul>
        </nav>

        <!-- compte btn -->
        <button class="flex items-center justify-center px-7 max-h-1 border-2 border-[#ffffff] p-4 font-bold font-['Sen']">Compte</button>
    </header>

    <!-- Content -->
    <main class="pr-2 grid h-screen" style="grid-template-columns : 200px 1fr;">
        <!-- Aside -->
        <div class="pr-2">
            <aside class="flex justify-center h-full bg-[#A23145] text-[#ffffff]  ">
                <h1 class="text-3xl">ACTUS</h1>
            </aside>
        </div>
        
        <!-- content -->
        <div class="h-full flex flex-col gap-2">
            <!-- Fil d'Ariane -->
            <nav class="flex items-center text-[#A23145] space-x-2 p-4 border-4 bg-white border-[#A23145] max-h-1 drop-shadow-[0_5px_5px_rgba(0,0,0,0.3)]">
                {% for breadcrumb in breadcrumbs %}
                    {% if loop.last %}
                        <span>{{ breadcrumb.title }}</span>
                    {% else %}
                        <a href="{{ breadcrumb.url }}" class="border-r-2 border-[#bfbfbf]  pr-2">{{ breadcrumb.title }}</a>
                    {% endif %}
                {% endfor %}
            </nav>

            <div>

            </div>
        </div>
    </main>
    

